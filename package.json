    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cpy-cli": "^2.0.0",
    "parcel-bundler": "^1.10.1",
    "postcss-modules": "^1.4.1",
    "rimraf": "^2.6.2",
    "serve": "^10.1.1"
  },
  "dependencies": {
    "is-image": "^2.0.0",
    "is-video": "^1.0.1",
    "lodash": "latest",
    "mobile-detect": "^1.4.3",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-human-timer": "^1.0.1",
    "react-plx": "^1.3.9",
