{
  "name": "egzaminy-prawo-jazdy",
  "version": "1.0.0",
  "description": "This is a web app containing most of the official Polish driving exam questions to help future drivers test their skills.",
  "scripts": {
    "movemedia": "cpy src/media dist/media",
